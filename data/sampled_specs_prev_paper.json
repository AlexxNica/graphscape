  {
    "description": "Corresponding to vis of (1900, Eastern)",
    "data": {"url": "data/cars.json"},
    "transform": {"filter": " datum.Year <= 1976 && datum.Horsepower < 140"},
    "mark": "point",
    "encoding": {
      "x": {"field": "Horsepower","type": "quantitative", "bin":true },
      "y": {
        "field": "Weight_in_lbs",
        "type": "quantitative",
        "aggregate": "mean"
